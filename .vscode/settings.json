{
  "breadcrumbs.enabled": true,
  "cSpell.enableFiletypes": ["rust"],
  "cSpell.ignorePaths": ["**/*.json"],
  "cSpell.words": [
    "actix",
    "authifier",
    "Bandcamp",
    "bincode",
    "bitfield",
    "bson",
    "chrono",
    "CLAMD",
    "COLOUR",
    "comedia",
    "customisation",
    "dashmap",
    "datetime",
    "deadqueue",
    "defn",
    "discrim",
    "dotenv",
    "dtls",
    "ffprobe",
    "Hasher",
    "HCAPTCHA",
    "Hmac",
    "idempotency",
    "impls",
    "indexmap",
    "Isahc",
    "Lightspeed",
    "linkify",
    "masq",
    "mediasoup",
    "moka",
    "Msgpack",
    "Onboarded",
    "oneshot",
    "openapi",
    "opengraph",
    "privkey",
    "pubkey",
    "querify",
    "querystring",
    "ratelimit",
    "Ratelimited",
    "ratelimiter",
    "repr",
    "reqwest",
    "rtcp",
    "rustc",
    "saudio",
    "schemars",
    "Screenshare",
    "screenshareaudio",
    "screensharevideo",
    "sctp",
    "semaphone",
    "SRTP",
    "substr",
    "svideo",
    "tailable",
    "tungstenite",
    "Unreact",
    "unreads",
    "untuple",
    "vergen",
    "VOSO",
    "webrtc"
  ],
  "code-runner.clearPreviousOutput": true,
  "code-runner.executorMap": {
    "rust": "cargo run # $fileName"
  },
  "editor.formatOnSave": true,
  "nixEnvSelector.nixFile": "${workspaceRoot}/default.nix",
  "nixEnvSelector.suggestion": false,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.linkedProjects": ["./Cargo.toml", "./Cargo.toml"],
  "rust-analyzer.showUnlinkedFileNotification": false
}
